<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeActivity"
    android:orientation="vertical">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="310dp"
        app:srcCompat="@drawable/member" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginHorizontal="25dp"
        android:gravity="center"
        android:layout_marginTop="50dp"
       >

        <TextView
            android:id="@+id/textView1_3"
            style="@style/textview1"
            android:gravity="right"
             />

        <EditText
            android:id="@+id/editTextId1_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="text"
            android:gravity="left"
             />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginHorizontal="25dp"
        android:gravity="center"
        android:layout_marginTop="5dp"
        >

        <TextView
            android:id="@+id/textView3_3"
            style="@style/textview3"
            android:gravity="right"
            />

        <EditText
            android:id="@+id/editTextName3_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="text"
            android:gravity="left"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginHorizontal="25dp"
        android:gravity="center"
        android:layout_marginTop="5dp"
        >

        <TextView
            android:id="@+id/textView4_3"
            style="@style/textview4"
            android:gravity="right"
            />
        <EditText
            android:id="@+id/editTextAge4_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="text"
            android:gravity="left"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginHorizontal="25dp"
        android:gravity="center"
        android:layout_marginTop="5dp"
        >

        <TextView
            android:id="@+id/textView5_3"
            style="@style/textview5"
            android:gravity="right"
            />

        <EditText
            android:id="@+id/editTextMbti5_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="text"
            android:gravity="left"
            />
    </LinearLayout>
    <Button
        android:id="@+id/btnlogin3_3"
        style="@style/button3"
        android:layout_marginHorizontal="25dp"
        android:layout_marginTop="60dp"
        android:onClick="doOnBtnClick"
         />
</LinearLayout>